# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (Optional)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Other LLM Providers (Optional)
# COHERE_API_KEY=your_cohere_api_key_here

# Application Settings
MADF_ENV=development
MADF_LOG_LEVEL=INFO
MADF_CONFIG_PATH=config/default.yaml

# Storage Settings
MADF_STATE_PATH=./.madf_state
MADF_OUTPUT_PATH=./output

# Performance Settings
MADF_MAX_AGENTS=10
MADF_ENABLE_CACHE=true

# Monitoring (Optional)
# MADF_METRICS_ENABLED=true
# MADF_METRICS_PORT=9090